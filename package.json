{
    "main": "dist/src/index.js",
    "name": "@threefoldjimber/digitaltwin-backend",
    "version": "1.0.7",
    "description": "Digitaltwin backend",
    "scripts": {
        "prepublish": "tsc -d --declarationDir dist/lib --declarationMap ",
        "dev": "concurrently 'tsc --watch' 'nodemon dist/src/server.js' 'node extensions/rmb.js'",
        "serve": "concurrently 'tsc --watch --preserveWatchOutput' 'nodemon dist/src/server.js' 'node extensions/rmb.js'",
        "build": "tsc",
        "migrate": "tsc && node ./dist/migrator/migrator.js",
        "prepare": "husky install"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/*/"
    ],
    "dependencies": {
        "@threefoldjimber/threefold_login": "^1.4.0",
        "@types/express-fileupload": "^1.1.6",
        "adm-zip": "^0.5.5",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "chalk": "^4.1.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "express-session": "^1.17.1",
        "faker": "^5.1.0",
        "file-type": "^16.5.3",
        "fs-extra": "^10.0.0",
        "http-status-codes": "^2.1.4",
        "imagemagick": "^0.1.3",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "mime": "^2.5.2",
        "moment": "^2.29.1",
        "morgan": "^1.10.0",
        "redis": "^4.0.3",
        "socket.io": "^3.1.0",
        "stream-buffers": "^3.0.2",
        "sync-request": "^6.1.0",
        "tsoa": "^3.7.0",
        "tweetnacl": "^1.0.3"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.4.34",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/fs-extra": "^9.0.11",
        "@types/imagemagick": "0.0.30",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/lodash": "^4.14.168",
        "@types/mime": "^2.0.3",
        "@types/morgan": "^1.9.2",
        "@types/socket.io": "^2.1.12",
        "@types/socket.io-client": "^1.4.35",
        "@types/stream-buffers": "^3.0.3",
        "concurrently": "^5.3.0",
        "husky": "^7.0.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.4.1",
        "pretty-quick": "^3.1.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "winston": "^3.3.3"
    }
